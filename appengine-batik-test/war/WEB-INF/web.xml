<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>svgUpload</servlet-name>
		<servlet-class>com.google.code.batik.test.SvgUploadHandler</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>svgUpload</servlet-name>
		<url-pattern>/svgUpload</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>imgUpload</servlet-name>
		<servlet-class>com.google.code.batik.test.ImgUploadHandler</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>imgUpload</servlet-name>
		<url-pattern>/imgUpload</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>serve</servlet-name>
		<servlet-class>com.google.code.batik.test.ImgServeHandler</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>serve</servlet-name>
		<url-pattern>/serve</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
